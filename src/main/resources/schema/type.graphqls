"Custom Scalar Date"
scalar DateTime

type Wish {
  id: ID!
  #author: User!
  title: String!
  markdown: String!
  #tags: [String!]!
  coverImageUrl: String
  #votes: [Vote!]!
  #comments: [Comment!]!
  createdAt: DateTime
  updatedAt: DateTime
}

type WishUpdate {
  success: Boolean!
  wish: Wish
}

type User {
  id: ID!
  userName: String!
  userProfileImage: String
  createdAt: DateTime
  updatedAt: DateTime
  wishList: [Wish!]!
  feedTags: [String!]!
}

type Vote{
  id: ID!
  itemId: ID!
  itemType: ItemType!
}

type Comment{
  id: ID!
  targetID: ID!
  userID:ID!
  message: String
  votes: [Vote!]!
  comments: [Comment!]!
  createdAt: DateTime
  updatedAt: DateTime
}
