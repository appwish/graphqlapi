input WishInput{
  title: String!
  markdown: String!
  #tags: [String!]!
  coverImageUrl: String
}

input VoteInput{
  itemId: ID!
  itemType: ItemType!
  voteType: VoteType!
}

input VoteSelector {
  itemId: ID!
  itemType: ItemType!
}

enum ItemType{
  WISH, COMMENT
}

enum VoteType{
  UP, DOWN
}
